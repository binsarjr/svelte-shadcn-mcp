{"name": "Button", "description": "Displays a button or a component that looks like a button", "category": "form", "props": "{\"variant\": {\"type\": \"'default' | 'destructive' | 'outline' | 'secondary' | 'ghost' | 'link'\", \"default\": \"'default'\"}, \"size\": {\"type\": \"'default' | 'sm' | 'lg' | 'icon'\", \"default\": \"'default'\"}, \"disabled\": {\"type\": \"boolean\", \"default\": \"false\"}, \"class\": {\"type\": \"string\", \"default\": \"''\"}, \"type\": {\"type\": \"'button' | 'submit' | 'reset'\", \"default\": \"'button'\"}}", "usage": "<Button variant=\"outline\" size=\"lg\">Click me</Button>", "installation": "npx shadcn-svelte@latest add button", "variants": "[\"default\", \"destructive\", \"outline\", \"secondary\", \"ghost\", \"link\"]", "dependencies": "[\"class-variance-authority\", \"clsx\", \"tailwind-merge\"]"}
{"name": "Input", "description": "Displays a form input field", "category": "form", "props": "{\"type\": {\"type\": \"string\", \"default\": \"'text'\"}, \"placeholder\": {\"type\": \"string\", \"default\": \"''\"}, \"disabled\": {\"type\": \"boolean\", \"default\": \"false\"}, \"value\": {\"type\": \"string\", \"default\": \"''\"}, \"class\": {\"type\": \"string\", \"default\": \"''\"}}", "usage": "<Input type=\"email\" placeholder=\"Enter your email\" bind:value={email} />", "installation": "npx shadcn-svelte@latest add input", "variants": "[\"default\"]", "dependencies": "[\"clsx\", \"tailwind-merge\"]"}
{"name": "Card", "description": "Displays a card with header, content, and footer", "category": "layout", "props": "{\"class\": {\"type\": \"string\", \"default\": \"''\"}}", "usage": "<Card.Root>\n  <Card.Header>\n    <Card.Title>Card Title</Card.Title>\n    <Card.Description>Card description</Card.Description>\n  </Card.Header>\n  <Card.Content>\n    <p>Card content goes here</p>\n  </Card.Content>\n  <Card.Footer>\n    <Button>Action</Button>\n  </Card.Footer>\n</Card.Root>", "installation": "npx shadcn-svelte@latest add card", "variants": "[\"default\"]", "dependencies": "[\"clsx\", \"tailwind-merge\"]"}
{"name": "Dialog", "description": "A window overlaid on either the primary window or another dialog window", "category": "overlay", "props": "{\"open\": {\"type\": \"boolean\", \"default\": \"false\"}, \"onOpenChange\": {\"type\": \"function\", \"default\": \"undefined\"}}", "usage": "<Dialog.Root bind:open={dialogOpen}>\n  <Dialog.Trigger>\n    <Button>Open Dialog</Button>\n  </Dialog.Trigger>\n  <Dialog.Content>\n    <Dialog.Header>\n      <Dialog.Title>Dialog Title</Dialog.Title>\n      <Dialog.Description>Dialog description</Dialog.Description>\n    </Dialog.Header>\n    <p>Dialog content</p>\n    <Dialog.Footer>\n      <Button on:click={() => dialogOpen = false}>Close</Button>\n    </Dialog.Footer>\n  </Dialog.Content>\n</Dialog.Root>", "installation": "npx shadcn-svelte@latest add dialog", "variants": "[\"default\"]", "dependencies": "[\"bits-ui\", \"clsx\", \"tailwind-merge\"]"}
